    <Function DataType="Interfaces.SabreAPI.FlightsResult" Name="GetFlights" Description="" id="idySN07PrNMW">
      <Arguments id="idDCZ3OZRjex">
        <ArgumentPair Name="origin" DataType="string" id="idxhVQTlFPbX" />
        <ArgumentPair Name="destination" DataType="string" id="idtiu4BdReiQ" />
        <ArgumentPair Name="departuredate" DataType="string" id="idWAv37vKqzu" />
        <ArgumentPair Name="returndate" DataType="string" id="idkpohCelLVt" />
        <ArgumentPair Name="limit" DataType="int" id="idU2EMXwGZw0" />
        <ArgumentPair Name="token" DataType="string" id="idnBp93JujOd" />
      </Arguments>
      <ImplementationDetails HttpType="GET">
        <HttpHeader Name="Authorization" Value="&quot;Bearer &quot; + token" />
        <GetUrlFunction><![CDATA[function string getUrl(
	string origin,
	string destination,
	string departuredate,
	string returndate,
	int limit,
	string token
)
{
 	return string.Format("/v1/shop/flights?origin={0}&destination={1}&departuredate={2}&returndate={3}&limit={4}", 
                         origin, destination, departuredate, returndate, limit);
}]]></GetUrlFunction>
      </ImplementationDetails>
    </Function>
